{
  "PolicyName": "root",
  "PolicyDocument": {
    "Version": "2012-10-17",
    "Statement": [
      {
        "Sid": "IamListAccess",
        "Effect": "Deny",
        "Action": [
          "iam:ListRoles",
          "iam:ListUsers"
        ],
        "Resource": "teas"
      },
      {
        "Sid": "S3ListAccess",
        "Effect": "Deny",
        "Action": [
          "s3:ListBucket",
          "s3:ListBucketVersions",
          "s3:ListBucketMultipartUploads"
        ],
        "Resource": [
          "arn:aws:s3:::confidential-data/*",
          "test"
        ]
      }, {
        "Sid": "S3ListAccess",
        "Effect": "Deny",
        "Action": [
          "s3:ListBucket",
          "s3:ListBucketVersions",
          "s3:ListBucketMultipartUploads"
        ],
        "Resource": [
          "arn:aws:s3:::confidential-data/*",
          "dsa"
        ]
      }
    ]
  }
}