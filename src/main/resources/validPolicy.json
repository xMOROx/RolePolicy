{
  "PolicyName": "root",
  "PolicyDocument": {
    "Version": "2012-10-17",
    "Statement": [
      {
        "Sid": "IamListAccess",
        "Effect": "Deny",
        "Action": [
          "iam:ListRoles",
          "iam:ListUsers"
